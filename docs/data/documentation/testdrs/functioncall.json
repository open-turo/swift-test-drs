      "content": [
        {
          "anchor": "overview",
          "level": 2,
          "text": "Overview",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This struct represents a single function call, storing information about what function was called,",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "with what parameters, and when it was called. It\u2019s used by the spy system to record function calls",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "so they can be verified later in tests.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "This struct is marked as ",
                  "type": "text"
                },
                {
                  "code": "@unchecked Sendable",
                  "type": "codeVoice"
                },
                {
                  "text": " even though the ",
                  "type": "text"
                },
                {
                  "code": "input",
                  "type": "codeVoice"
                },
                {
                  "text": " might not actually be ",
                  "type": "text"
                },
                {
                  "code": "Sendable",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "The risk is minimal since this is designed to be used with tests and SwiftUI previews.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Note",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
